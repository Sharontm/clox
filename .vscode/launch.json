        "version": "0.2.0",
        "configurations": [
          {
            "name": "C/C++ Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "C/C++: gcc build active file"
          },
          {
            "name": "C/C++ Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb"
          }
        ]
}